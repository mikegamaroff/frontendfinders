generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model submissions {
  id          Int     @id @default(autoincrement())
  name        String? @db.VarChar(45)
  email       String? @db.VarChar(45)
  description String? @db.VarChar(45)
  phone       String? @db.VarChar(45)

}
